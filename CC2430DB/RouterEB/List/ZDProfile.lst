   1269          
   1270              NetworkList = NetworkList->nextDesc;    // Move to next list entry
   1271            }
   1272          
   1273            FillAndSendBuffer( &TransSeq, dstAddr, Mgmt_NWK_Disc_rsp, len, buf );
   1274          }
