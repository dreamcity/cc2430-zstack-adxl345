    768          /*********************************************************************
    769           * @fn      rxCB
    770           *
    771           * @brief   Process UART Rx event handling.
    772           *          May be triggered by an Rx timer expiration - less than max
    773           *          Rx bytes have arrived within the Rx max age time.
    774           *          May be set by failure to alloc max Rx byte-buffer for the DMA Rx -
    775           *          system resources are too low, so set flow control?
    776           *
    777           * @param   none
    778           *
    779           * @return  none
    780           */
    781          static void rxCB( uint8 port, uint8 event )
    782          {
    783            uint8 *buf, len;
    784          
    785            /* While awaiting retries/response, only buffer 1 next buffer: otaBuf2.
    786             * If allow the DMA Rx to continue to run, allocating Rx buffers, the heap
    787             * will become so depleted that an incoming OTA response cannot be received.
    788             * When the Rx data available is not read, the DMA Rx Machine automatically
    789             * sets flow control off - it is automatically re-enabled upon Rx data read.
    790             * When the back-logged otaBuf2 is sent OTA, an Rx data read is scheduled.
    791             */
    792            if ( otaBuf2 )
    793            {
    794              return;
    795            }
    796          
    797            if ( !(buf = osal_mem_alloc( SERIAL_APP_RX_CNT )) )
    798            {
    799              return;
    800            }
    801          
    802            /* HAL UART Manager will turn flow control back on if it can after read.
    803             * Reserve 1 byte for the 'sequence number'.
    804             */
    805            len = HalUARTRead( port, buf+1, SERIAL_APP_RX_CNT-1 );
    806          
    807            if ( !len )  // Length is not expected to ever be zero.
    808            {
    809              osal_mem_free( buf );
    810              return;
    811            }
    812          
    813            /* If the local global otaBuf is in use, then either the response handshake
    814             * is being awaited or retries are being attempted. When the wait/retries
    815             * process has been exhausted, the next OTA msg will be attempted from
    816             * otaBuf2, if it is not NULL.
    817             */
    818            if ( otaBuf )
    819            {
    820              otaBuf2 = buf;
    821              otaLen2 = len;
    822            }
    823            else
    824            {
    825              otaBuf = buf;
    826              otaLen = len;
    827              /* Don't call SerialApp_SendData() from here in the callback function.
    828               * Set the event so SerialApp_SendData() runs during this task's time slot.
    829               */
    830              osal_set_event( SerialApp_TaskID, SERIALAPP_MSG_SEND_EVT );
    831            }
    832          }
