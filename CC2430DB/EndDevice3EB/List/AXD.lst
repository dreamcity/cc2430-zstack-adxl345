    781          /*********************************************************************
    782           * @fn      rxCB
    783           *
    784           * @brief   Process UART Rx event handling.
    785           *          May be triggered by an Rx timer expiration - less than max
    786           *          Rx bytes have arrived within the Rx max age time.
    787           *          May be set by failure to alloc max Rx byte-buffer for the DMA Rx -
    788           *          system resources are too low, so set flow control?
    789           *
    790           * @param   none
    791           *
    792           * @return  none
    793           */
    794          static void rxCB( uint8 port, uint8 event )
    795          {
    796            uint8 *buf, len;
    797          
    798            /* While awaiting retries/response, only buffer 1 next buffer: otaBuf2.
    799             * If allow the DMA Rx to continue to run, allocating Rx buffers, the heap
    800             * will become so depleted that an incoming OTA response cannot be received.
    801             * When the Rx data available is not read, the DMA Rx Machine automatically
    802             * sets flow control off - it is automatically re-enabled upon Rx data read.
    803             * When the back-logged otaBuf2 is sent OTA, an Rx data read is scheduled.
    804             */
    805            if ( otaBuf2 )
    806            {
    807              return;
    808            }
    809          
    810            if ( !(buf = osal_mem_alloc( SERIAL_APP_RX_CNT )) )
    811            {
    812              return;
    813            }
    814          
    815            /* HAL UART Manager will turn flow control back on if it can after read.
    816             * Reserve 1 byte for the 'sequence number'.
    817             */
    818            len = HalUARTRead( port, buf+1, SERIAL_APP_RX_CNT-1 );
    819          
    820            if ( !len )  // Length is not expected to ever be zero.
    821            {
    822              osal_mem_free( buf );
    823              return;
    824            }
    825          
    826            /* If the local global otaBuf is in use, then either the response handshake
    827             * is being awaited or retries are being attempted. When the wait/retries
    828             * process has been exhausted, the next OTA msg will be attempted from
    829             * otaBuf2, if it is not NULL.
    830             */
    831            if ( otaBuf )
    832            {
    833              otaBuf2 = buf;
    834              otaLen2 = len;
    835            }
    836            else
    837            {
    838              otaBuf = buf;
    839              otaLen = len;
    840              /* Don't call SerialApp_SendData() from here in the callback function.
    841               * Set the event so SerialApp_SendData() runs during this task's time slot.
    842               */
    843              osal_set_event( SerialApp_TaskID, SERIALAPP_MSG_SEND_EVT );
    844            }
    845          }
    846          #endif
