    469          void AXD_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    470          {
    471            switch ( inMsg->clusterID )
    472            {
    473              case End_Device_Bind_rsp:
    474                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    475                {
    476                  // Light LED
    477                  HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    478                }
    479          #if defined(BLINK_LEDS)
    480                else
    481                {
    482                  // Flash LED to show failure
    483                  HalLedSet ( HAL_LED_4, HAL_LED_MODE_FLASH );
    484                }
    485          #endif
    486                break;
    487          
    488              case Match_Desc_rsp:
    489                {
    490                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    491                  if ( pRsp )
    492                  {
    493                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    494                    {
    495                      AXD_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    496                      AXD_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    497                      // Take the first endpoint, Can be changed to search through endpoints
    498                      AXD_DstAddr.endPoint = pRsp->epList[0];
    499          
    500                      // Light LED
    501                      HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    502                    }
    503                    osal_mem_free( pRsp );
    504                  }
    505                }
    506                break;
    507            }
    508          }
