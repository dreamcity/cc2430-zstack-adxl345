    257          /*********************************************************************
    258           * @fn      AXD_Init
    259           *
    260           * @brief   Initialization function for the Generic App Task.
    261           *          This is called during initialization and should contain
    262           *          any application specific initialization (ie. hardware
    263           *          initialization/setup, table initialization, power up
    264           *          notificaiton ... ).
    265           *
    266           * @param   task_id - the ID assigned by OSAL.  This ID should be
    267           *                    used to send messages and set timers.
    268           *
    269           * @return  none
    270           */
    271          void AXD_Init( byte task_id )
    272          {
    273            halUARTCfg_t uartConfig;
    274            AXD_TaskID = task_id;
    275            AXD_NwkState = DEV_INIT;
    276            AXD_TransID = 0;
    277          //  AXD_TransID2 = 1;
    278          
    279            // Device hardware initialization can be added here or in main() (Zmain.c).
    280            // If the hardware is application specific - add it here.
    281            // If the hardware is other parts of the device add it in main().
    282          
    283            AXD_DstAddr.addrMode = (afAddrMode_t)AddrNotPresent;
    284            AXD_DstAddr.endPoint = 0;
    285            AXD_DstAddr.addr.shortAddr = 0;
    286          
    287            // Fill out the endpoint description.
    288            AXD_epDesc.endPoint = AXD_ENDPOINT;
    289            AXD_epDesc.task_id = &AXD_TaskID;
    290            AXD_epDesc.simpleDesc
    291                      = (SimpleDescriptionFormat_t *)&AXD_SimpleDesc;
    292            AXD_epDesc.latencyReq = noLatencyReqs;
    293          
    294            // Register the endpoint description with the AF
    295            afRegister( &AXD_epDesc );
    296          
    297            // Register for all key events - This app will handle all key events
    298            RegisterForKeys( AXD_TaskID );
    299            uartConfig.configured           = TRUE;              // 2430 don't care.
    300            uartConfig.baudRate             = SERIAL_APP_BAUD;
    301            uartConfig.flowControl          = TRUE;
    302            uartConfig.flowControlThreshold = SERIAL_APP_THRESH;
    303            uartConfig.rx.maxBufSize        = SERIAL_APP_RX_MAX;
    304            uartConfig.tx.maxBufSize        = SERIAL_APP_TX_MAX;
    305            uartConfig.idleTimeout          = SERIAL_APP_IDLE;   // 2430 don't care.
    306            uartConfig.intEnable            = TRUE;              // 2430 don't care.
    307          #if SERIAL_APP_LOOPBACK
    308            uartConfig.callBackFunc         = rxCB_Loopback;
    309          #else
    310            uartConfig.callBackFunc         = rxCB;
    311          #endif
    312            HalUARTOpen (SERIAL_APP_PORT, &uartConfig);
    313          
    314            // Update the display
    315          #if defined ( LCD_SUPPORTED )
    316              HalLcdWriteString( "AXD", HAL_LCD_LINE_1 );
    317          #endif
    318          
    319            ZDO_RegisterForZDOMsg( AXD_TaskID, End_Device_Bind_rsp );
    320            ZDO_RegisterForZDOMsg( AXD_TaskID, Match_Desc_rsp );
    321            
    322            AXD_Group.ID = AXD_GROUP;
    323            aps_AddGroup(AXD_ENDPOINT,&AXD_Group);
    324            //下面是用户自定义的初始化
    325          #ifdef AXD_END
    326            Init_ADXL345();
    327          #endif
    328          }
