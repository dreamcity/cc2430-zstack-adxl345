    330          /*********************************************************************
    331           * @fn      AXD_ProcessEvent
    332           *
    333           * @brief   Generic Application Task event processor.  This function
    334           *          is called to process all events for the task.  Events
    335           *          include timers, messages and any other user defined events.
    336           *
    337           * @param   task_id  - The OSAL assigned task ID.
    338           * @param   events - events to process.  This is a bit map and can
    339           *                   contain more than one event.
    340           *
    341           * @return  none
    342           */
    343          UINT16 AXD_ProcessEvent( byte task_id, UINT16 events )
    344          {
    345            
    346            afIncomingMSGPacket_t *MSGpkt;
    347          //  osal_event_hdr_t *pMsg;
    348            afDataConfirm_t *afDataConfirm;
    349          
    350            // Data Confirmation message fields
    351            byte sentEP;
    352            ZStatus_t sentStatus;
    353            byte sentTransID;       // This should match the value sent
    354          
    355            if ( events & SYS_EVENT_MSG )
    356            {
    357              MSGpkt = (afIncomingMSGPacket_t *)osal_msg_receive( AXD_TaskID );
    358              while ( MSGpkt )
    359              {
    360                switch ( MSGpkt->hdr.event )
    361                {
    362                  case ZDO_CB_MSG:
    363                    AXD_ProcessZDOMsgs( (zdoIncomingMsg_t *)MSGpkt );
    364                    break;
    365          
    366                  case KEY_CHANGE:
    367                    AXD_HandleKeys( ((keyChange_t *)MSGpkt)->state, ((keyChange_t *)MSGpkt)->keys );
    368                    break;
    369          
    370                  case AF_DATA_CONFIRM_CMD:
    371                    // This message is received as a confirmation of a data packet sent.
    372                    // The status is of ZStatus_t type [defined in ZComDef.h]
    373                    // The message fields are defined in AF.h
    374                    afDataConfirm = (afDataConfirm_t *)MSGpkt;
    375                    sentEP = afDataConfirm->endpoint;
    376                    sentStatus = afDataConfirm->hdr.status;
    377                    sentTransID = afDataConfirm->transID;
    378                    (void)sentEP;
    379                    (void)sentTransID;
    380          
    381                    // Action taken when confirmation is received.
    382                    if ( sentStatus != ZSuccess )
    383                    {
    384                      // The data wasn't delivered -- Do something
    385                    }
    386                    break;
    387          
    388                  case AF_INCOMING_MSG_CMD:
    389              //      MSGpkt = (afIncomingMSGPacket_t *) pMsg;
    390          #ifdef AXD_COR
    391                    //AXD_ReceiveDataIndication( MSGpkt->srcAddr.addr.shortAddr, MSGpkt->clusterId,
    392                    //                          MSGpkt->cmd.DataLength, MSGpkt->cmd.Data);
    393                    AXD_MessageMSGCB( MSGpkt );
    394          #endif
    395                    break;
    396          
    397                  case ZDO_STATE_CHANGE:
    398                    AXD_NwkState = (devStates_t)(MSGpkt->hdr.status);
    399                    if ( (AXD_NwkState == DEV_ZB_COORD)
    400                        || (AXD_NwkState == DEV_ROUTER)
    401                        || (AXD_NwkState == DEV_END_DEVICE) )
    402                    {
    403                      // Start sending "the" message in a regular interval.
    404                      osal_start_timerEx( AXD_TaskID,
    405                                          AXD_SEND_MSG_EVT,
    406                                          AXD_SEND_MSG_TIMEOUT );
    407                    }
    408                    break;
    409          
    410                  default:
    411                    break;
    412                }
    413          
    414                // Release the memory
    415                osal_msg_deallocate( (uint8 *)MSGpkt );
    416          
    417                // Next
    418                MSGpkt = (afIncomingMSGPacket_t *)osal_msg_receive( AXD_TaskID );
    419              }
    420          
    421              // return unprocessed events
    422              return (events ^ SYS_EVENT_MSG);
    423            }
    424          
    425            // Send a message out - This event is generated by a timer
    426            //  (setup in AXD_Init()).
    427            if ( events & AXD_SEND_MSG_EVT )
    428            {
    429              //zb_HandleOsalEvent(events);
    430              AXD_SendTheMessage();
    431              // Setup to send message again
    432          #ifdef AXD_ROUTER
    433              test2++;
    434          #endif
    435          #ifdef AXD_END2
    436              test2--;
    437          #endif
    438              osal_start_timerEx( AXD_TaskID,
    439                                  AXD_SEND_MSG_EVT,
    440                                  (AXD_SEND_MSG_TIMEOUT/5) );
    441          
    442              // return unprocessed events
    443              return (events ^ AXD_SEND_MSG_EVT);
    444            }
    445              if ( events & ( ZB_USER_EVENTS ) )
    446            {
    447              // User events are passed to the application
    448              zb_HandleOsalEvent( events );
    449          
    450              // Do not return here, return 0 later
    451            }
    452          
    453            // Discard unknown events
    454            return 0;
    455          }
